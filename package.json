{
	"name": "@candidosales/svelte-google-translate",
	"description": "Google Translate component for Svelte",
	"version": "0.0.10",
	"author": {
		"email": "candidosg@gmail.com",
		"name": "CÃ¢ndido Sales Gomes"
	},
	"scripts": {
		"dev": "vite dev",
		"preview": "vite preview",
		"build": "vite build && svelte-kit sync && svelte-package",
		"build:publish": "npm run build && npm publish",
		"sync": "svelte-kit sync",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"package": "svelte-package"
	},
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"svelte": "./dist/index.js",
	"keywords": [
		"svelte",
		"sveltekit",
		"google-translate"
	],
	"dependencies": {
		"svelte": "^3.57.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "next",
		"@sveltejs/package": "^2.0.2",
		"@typescript-eslint/eslint-plugin": "^5.56.0",
		"@typescript-eslint/parser": "^5.56.0",
		"eslint": "^8.36.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.8.6",
		"prettier-plugin-svelte": "^2.10.0",
		"svelte-check": "^3.1.4",
		"svelte2tsx": "^0.6.10",
		"tslib": "^2.5.0",
		"typescript": "^5.0.2",
		"vite": "^4.2.1"
	},
	"type": "module",
	"bugs": {
		"url": "https://github.com/candidosales/svelte-google-translate/issues"
	},
	"license": "MIT"
}
